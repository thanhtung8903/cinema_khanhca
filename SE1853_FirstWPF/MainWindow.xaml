<Window x:Class="SE1853_FirstWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SE1853_FirstWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Activated="Window_Activated" WindowStartupLocation="CenterScreen">
    <Grid>
        <DockPanel>
            <Menu>
                <MenuItem Header="Films" Name="mnuFilm" IsEnabled="False" Click="mnuFilm_Click"></MenuItem>
                <MenuItem Header="Shows" Name="mnuShow" IsEnabled="False" Click="mnuShows_Click"></MenuItem>
                <MenuItem Header="Login" Name="mnuLogin" Click="mnuLogin_Click"></MenuItem>
            </Menu>
        </DockPanel>
        
    </Grid>
</Window>
